{
  "name": "mithril-ssr",
  "version": "2.1.0",
  "description": "ssr",
  "repository": "https://github.com/souricevincent/mithril-ssr-boilerplate.git",
  "sideEffects": false,
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:prod": "rm -rf build && webpack --mode production --config webpack.config.js",
    "build:dev": "rm -rf build && webpack --mode development --config webpack.config.js",
    "watch": "rm -rf build && webpack --watch --mode development --config webpack.config.js",
    "start": "nodemon build/server.js",
    "lint": "eslint 'src/**'",
    "lint:fix": "eslint 'src/**' --fix",
    "webpack": "webpack --mode development --config webpack.config.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/koa": "^2.11.4",
    "@types/koa-route": "^3.2.4",
    "@types/koa-static": "^4.0.1",
    "@types/lodash": "^4.14.161",
    "@types/mithril": "^2.0.3",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.0.0",
    "circular-dependency-plugin": "^5.2.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-unicorn": "^21.0.0",
    "meiosis-tracer": "^3.0.0",
    "nodemon-webpack-plugin": "^4.3.2",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.0",
    "webpack-shell-plugin-alt": "^1.4.0",
    "ts-loader": "^8.0.11"
  },
  "dependencies": {
    "@urql/core": "^1.15.0",
    "graphql": "^15.2.0",
    "graphql-tag": "^2.11.0",
    "koa": "^2.13.0",
    "koa-route": "^3.2.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.20",
    "mithril": "^2.0.4",
    "mithril-node-render": "^3.0.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.6",
    "wonka": "^4.0.14"
  },
  "nodemonConfig": {
    "inspect": true,
    "watch": [
      "build"
    ],
    "ignore": [
      "build/assets/*"
    ],
    "delay": "0"
  }
}
